<body id="<%= id %>" class="body-blue">
    <div class="page-wrapper">
        <!-- ******Header****** -->
        <header id="header" class="header">
            <div id="back-to-zube">
                <a href="/"><i class="zubeicon-home"></i> Back to Zube</a>
            </div>
            <div class="container">
                <div class="branding">
                    <h1 class="logo">
                        <a href="/docs">
                            <span aria-hidden="true" class="zubeicon-zube-mark icon"></span>
                            <span class="text-bold">Zube</span> <span class="text-highlight">Docs</span>
                        </a>
                    </h1>
                </div><!--//branding-->
                <ol class="breadcrumb">
                    <li><a href="/docs">Home</a></li>
                    <li class="active">API Documentation</li>
                </ol>
            </div><!--//container-->
        </header><!--//header-->
        <div class="doc-wrapper">
            <div class="container">
                <div id="doc-header" class="doc-header text-center">
                    <h1 class="doc-title"><i class="icon fa fa-paper-plane"></i> API Documentation</h1>
                    <div class="meta"><i class="fa fa-clock-o"></i> Last updated: <%= date %></div>
                </div><!--//doc-header-->
                <div class="doc-body">
                    <div class="doc-content">
                        <div class="content-inner">
                            <%- documentationContent %>
                            <div class="section-block">
                                <div class="callout-block contact-callout">
                                    <div class="content">
                                        <p>
                                            Have more questions? Need help setting up your workflow? Get in touch!
                                        </p>
                                        <p>
                                            <a href="mailto:team@zube.io" target="_top"><i class="zubeicon-envelope"></i> team@zube.io</a>
                                            <a href="http://slack.zube.io" target="_blank"><i class="zubeicon-slack"></i> Zube on Slack</a>
                                        </p>
                                    </div><!--//content-->
                                </div><!--//callout-block-->
                            </div>
                        </div><!--//content-inner-->
                    </div><!--//doc-content-->
                    <%- sidebar %>
                </div><!--//doc-body-->
            </div><!--//container-->
        </div><!--//doc-wrapper-->

    </div><!--//page-wrapper-->

    <footer id="footer" class="footer text-center">
      <div>
        <ul class="list-inline">
          <li><a href="/">Home</a></li>
          <li>&sdot;</li>
          <li><a href="/contact">Contact</a></li>
          <li>&sdot;</li>
          <li><a href="/blog">Blog</a></li>
          <li>&sdot;</li>
          <li><a href="/tos">Terms and Privacy</a></li>
        </ul>
        <p class="copyright text-center medium">Copyright &copy; Pivit Inc. <span id="copyright-date"></span>. All Rights Reserved</p>
      </div>
    </footer><!--//footer-->

